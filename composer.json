{
    "name": "dicr/yii2-helper",
    "type": "yii2-extension",
    "license": "Apache-2.0",
    "description": "Набор вспомогательных утилит для Yii2",
    "keywords": [
        "php yii2 helper utilities"
    ],
    "authors": [
        {
            "name": "Igor Tarasov",
            "email": "develop@dicr.org",
            "homepage": "http://dicr.org",
            "role": "developer"
        }
    ],
    "homepage": "https://gitlab.com/dicr/yii2-helper",
    "support": {
        "source": "https://gitlab.com/dicr/yii2-helper.git"
    },
    "minimum-stability": "stable",
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-mbstring": "*",
        "ext-intl": "*",
        "yiisoft/yii2": "~2.0.29"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "8.4.1"
    },
    "autoload": {
        "psr-4": {
            "dicr\\helper\\": "src"
        }
    }
}
